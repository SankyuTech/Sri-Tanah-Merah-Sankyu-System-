Private Sub LV1_Click()
'on error resume next
LM_KEY = frm150.LV1.SelectedItem.Key

If LM_KEY = "License" Then
    
    Note = "Adakah anda yakin untuk daftarkan LICENSE bagi station ini?"
    
    Answer = MsgBox(Note, vbQuestion + vbYesNo, "Confirmation")
    
    If Answer = vbYes Then
        
        Call register_license
    
    End If

ElseIf LM_KEY = "Setting Invoice" Then

    Call MDI_frm1_unload_all_menu

    frm135.Show

ElseIf LM_KEY = "Invoice" Then

    Call MDI_frm1_unload_all_menu

    frm151.Show

ElseIf LM_KEY = "Reset Sistem" Then

    Note = "Adakah anda yakin untuk RESET DATABASE sistem ini?"
    
    Answer = MsgBox(Note, vbQuestion + vbYesNo, "Confirmation")
    
    If Answer = vbYes Then
        Call reset_database
    End If
    
End If

End Sub
