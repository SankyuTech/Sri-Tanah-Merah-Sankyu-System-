Sub Frm83_form_load()
'on error resume next
Call Frm83_Initial_Setting 'start here
Call Frm83_initial_setting2
Call Frm83_jurujual

Frm83.L41_Text = 2 '0 : Belian emas terpakai , 1 : Trade in , 2 : Barang baru

'If Frm16.CB1 = 1 Then
'    Call Frm96_initial
'    'Frm96.L1_Text = 0 '0 : Penerimaan Stok Baru , 1 : Edit
'    Frm96.Show
'End If
Exit Sub

If Frm16.CB2 = 1 Then
    Call Frm26_initial
    Call Frm27_initial
    Call Frm28_initial

    Frm16_LM_KATEGORI_PEMBELI = "Pelanggan / Pelanggan Biasa" 'Default
    
    If Frm16.CB3 = 1 Then
        Frm16_LM_KATEGORI_PEMBELI = "Pelanggan / Pelanggan Biasa"
        Frm83.L39_Text = 1
    End If
    If Frm16.CB4 = 1 Then
        Frm16_LM_KATEGORI_PEMBELI = "Member / Ahli"
        Frm83.L39_Text = 2
    End If
    If Frm16.CB5 = 1 Then
        Frm16_LM_KATEGORI_PEMBELI = "Pengedar"
        Frm83.L39_Text = 4
    End If
    If Frm16.CB6 = 1 Then
        Frm16_LM_KATEGORI_PEMBELI = "RAF"
        Frm83.L39_Text = 3
    End If
    If Frm16.CB7 = 1 Then
        Frm16_LM_KATEGORI_PEMBELI = "Normal Dealer (ND)"
        Frm83.L39_Text = 5
    End If
    If Frm16.CB8 = 1 Then
        Frm16_LM_KATEGORI_PEMBELI = "Master Dealer (MD)"
        Frm83.L39_Text = 6
    End If
    
    Note = "Barang yang akan diterima ini adalah dijual oleh [" & Frm16_LM_KATEGORI_PEMBELI & "] kepada pihak kedai." & vbCrLf & _
            vbNullString & vbCrLf & _
            "Jika terdapat perubahan pada kategori penjual sila buat pemilihan semula."
    
    Answer = MsgBox(Note, vbQuestion + vbYesNo, "Confirmation")
    
    If Answer = vbNo Then
        Exit Sub
    ElseIf Answer = vbYes Then
            
        Call Frm83_Reset_Form
        
        Frm83.L41_Text = 0 '0 : Belian emas terpakai , 1 : Trade in , 2 : Barang baru
        
        Call frm83_flag_barang_trade_in
        
        'If Frm16.CB4 = 1 Or Frm16.CB5 = 1 Or Frm16.CB6 = 1 Or Frm16.CB7 = 1 Or Frm16.CB8 = 1 Then
        '    Frm83.CMD17.Visible = True
        '    Frm83.CMD18.Visible = False
        '    Frm83.CMD19.Visible = True
        'End If
        
        Frm83.CBB1.AddItem "Trade In"
        Frm83.CBB1 = "Trade In"
        Frm83.TB1 = "TI"
        
        Frm83.CB2 = 1
        Frm83.CB3 = 0
        Frm83.CB11 = 0
        Frm83.CB12 = 0
        
        Frm83.CB2.Enabled = False
        Frm83.CB3.Enabled = False
        Frm83.CB11.Enabled = False
        Frm83.CB12.Enabled = False
        Frm83.Show
        
    End If
End If
End Sub
Sub Frm83_form_load_trade_in()
